@charset "UTF-8";
page {
  height: 100%;
  background-color: #F7F7F7;
}
.overflow2 {
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.overflow1 {
  overflow: hidden;
  /*超出部分隐藏*/
  white-space: nowrap;
  /*不换行*/
  text-overflow: ellipsis;
  /*超出部分文字以...显示*/
}
.overflow3 {
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  line-clamp: 3;
  -webkit-box-orient: vertical;
}
.mask {
  background-color: rgba(0, 0, 0, 0.6);
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  display: flex;
  align-items: center;
  justify-content: center;
}
.centerAll {
  /*上下左右都居中*/
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.ag {
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.ag {
  display: flex;
  justify-content: center;
  align-items: center;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.allBg {
  height: 100%;
  background-color: #F7F7F7;
}
.bgBox {
  height: 100%;
  background: #fff;
}
.bg {
  background: #F7F7F7;
  width: 100%;
  height: 100%;
  position: fixed;
}
.whiteBg {
  background: #fff;
}
.w100 {
  width: 100%;
}
.flex {
  display: flex;
}
.img28 {
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
  margin-right: 6rpx;
  margin-top: -4rpx;
}
.icon28 {
  width: auto;
  height: 28rpx;
  vertical-align: middle;
  margin-right: 6rpx;
  margin-top: -4rpx;
}
.mt20 {
  margin-top: 20rpx;
}
.mb20 {
  margin-bottom: 20rpx;
}
.goTop {
  border-radius: 100%;
  position: fixed;
  right: 28rpx;
  bottom: 186rpx;
  z-index: 9;
  width: 80rpx;
  height: 80rpx;
}
.goTop .goTop-img {
  width: 100%;
  height: 100%;
}
.flexBox {
  display: flex;
  flex-direction: column;
  height: 100%;
  /*高度必须指定*/
  overflow: hidden;
}
.boxScroll {
  flex: 1;
  overflow: scroll;
  /*必须写这一条*/
  height: 1px;
}
.blackBg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 199;
}
.goodsItem {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 16rpx;
}
.goodsItem .goodsImg {
  width: 240rpx;
  height: 240rpx;
  margin-right: 24rpx;
  border-radius: 10rpx;
}
.goodsItem .goodsMsg {
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  flex: 1;
  height: 240rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
}
.goodsItem .goodsMsg .goodsMsg-top ._h2 {
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 6rpx;
}
.goodsItem .goodsMsg .goodsMsg-top .goods-sal {
  font-size: 24rpx;
  color: #666;
}
.goodsItem .goodsMsg .goodsMsg-foot {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.goodsItem .goodsMsg .goodsMsg-foot .foot-lf {
  font-size: 26rpx;
  color: #FF3445;
}
.goodsItem .goodsMsg .goodsMsg-foot .foot-lf .big {
  font-size: 40rpx;
}
.goodsItem .goodsMsg .goodsMsg-foot .foot-lf .gray {
  color: #999;
  text-decoration: line-through;
  margin-left: 2rpx;
}
.goodsItem .goodsMsg .goodsMsg-foot .foot-rf {
  width: 60rpx;
  height: 60rpx;
}
.uni-load-more {
  display: flex;
  flex-direction: row;
  height: 40px;
  align-items: center;
  justify-content: center;
}
.uni-load-more__text {
  font-size: 14px;
  margin-left: 8px;
}
.uni-load-more__img {
  width: 24px;
  height: 24px;
}
.uni-load-more__img--nvue {
  color: #666666;
}
.uni-load-more__img--android,
.uni-load-more__img--ios {
  width: 24px;
  height: 24px;
  -webkit-transform: rotate(0deg);
          transform: rotate(0deg);
}
.uni-load-more__img--android {
  -webkit-animation: loading-ios 1s 0s linear infinite;
          animation: loading-ios 1s 0s linear infinite;
}
@-webkit-keyframes loading-android {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes loading-android {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
.uni-load-more__img--ios-H5 {
  position: relative;
  -webkit-animation: loading-ios-H5 1s 0s step-end infinite;
          animation: loading-ios-H5 1s 0s step-end infinite;
}
.uni-load-more__img--ios-H5 image {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}
@-webkit-keyframes loading-ios-H5 {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
8% {
    -webkit-transform: rotate(30deg);
            transform: rotate(30deg);
}
16% {
    -webkit-transform: rotate(60deg);
            transform: rotate(60deg);
}
24% {
    -webkit-transform: rotate(90deg);
            transform: rotate(90deg);
}
32% {
    -webkit-transform: rotate(120deg);
            transform: rotate(120deg);
}
40% {
    -webkit-transform: rotate(150deg);
            transform: rotate(150deg);
}
48% {
    -webkit-transform: rotate(180deg);
            transform: rotate(180deg);
}
56% {
    -webkit-transform: rotate(210deg);
            transform: rotate(210deg);
}
64% {
    -webkit-transform: rotate(240deg);
            transform: rotate(240deg);
}
73% {
    -webkit-transform: rotate(270deg);
            transform: rotate(270deg);
}
82% {
    -webkit-transform: rotate(300deg);
            transform: rotate(300deg);
}
91% {
    -webkit-transform: rotate(330deg);
            transform: rotate(330deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes loading-ios-H5 {
